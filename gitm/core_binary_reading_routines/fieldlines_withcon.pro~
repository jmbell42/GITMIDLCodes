n = 25722Le = 19683Lt = e*4+exyz = dblarr(16,n)ptemp = ulonarr(4,e)p = ulonarr(5,e)edges = ulonarr(t)openr,1,'test.iso'readf,1,xyzreadf,1,ptempclose,1p(0,*) = 4p(1:4,*) = ptemp-1edges = popoly = obj_new('IDLgrPolygon',xyz(0:2,*),polygon=edges)omodel = obj_new('IDLgrModel')oview = obj_new('IDLgrView')omodel->Add,opolyoview->Add,omodelovrml = obj_new('IDLgrVRML',filename='new.wrl',quality=2)ovrml->Draw,oviewobj_destroy, opolyobj_destroy, omodelobj_destroy, oviewobj_destroy, ovrmlend