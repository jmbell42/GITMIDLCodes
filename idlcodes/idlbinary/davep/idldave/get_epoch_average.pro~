PRO get_epoch_average,data,rtime,eptime,epochave,newtimearray

neptimes = 24/(eptime/60.)
newtimearray = intarr(3,neptimes)
for itime = 0,neptimes - 1 do begin
   mins = itime * eptime
   nhours = fix(nmins / 3600.)
   nmins =  (nmins/3600 - nhours) * 60.
   newtimearray = [nhours,nmins,0]
endfor

ntimes = n_elements(data)



end
