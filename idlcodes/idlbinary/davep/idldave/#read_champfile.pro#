;PRO read_champfile, file, itimearr, lat, lon, alt, mass, rho


nlines = filelines(file)
lat = fltarr(nlines)
lon = lat
alt = lat
mass = lat
rho = lat
mjd = lat
itimearr = intarr(6,nlines)

openr, 5, file
for iline = 0, nlines - 1 do begin
    readf, 5, mjd(iline), lat(iline), lon(iline), alt(iline), mass(iline), $
      rho(iline)
    mjd2000, mjd(iline), timearr
    itimearr(*,iline) = timearr


endfor 
