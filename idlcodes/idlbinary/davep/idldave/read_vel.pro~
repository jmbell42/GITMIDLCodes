close,1 
openr,1,'velocity.txt'
mt = 100000

oldn = fltarr(mt)
newn = fltarr(mt)
dv = fltarr(mt)
dnv = fltarr(mt)
diff = fltarr(mt)
v = fltarr(mt)
dn = fltarr(mt)

i = 0L
t = ' '
while not eof(1) do begin
    readf,1,t, oldn(i),newn(i),dv(i),dnv(i),diff(i),v(i),dn(i)
    i = i + 1
endwhile
close,1
nt = i - 1 
oldn = oldn(0:nt-1)
newn = newn(0:nt-1)
dv = dv(0:nt-1)
dnv =dnv(0:nt-1)
diff = diff(0:nt-1)
v = v(0:nt-1)
dn = dn(0:nt-1)


openr,1,'../run.100/velocity.txt'

oldn2 = fltarr(mt)
newn2 = fltarr(mt)
dv2 = fltarr(mt)
dnv2 = fltarr(mt)
diff2 = fltarr(mt)
v2 = fltarr(mt)
dn2 = fltarr(mt)

i = 0L
t = ' '
while not eof(1) do begin
    readf,1,t, oldn2(i),newn2(i),dv2(i),dnv2(i),diff2(i),v2(i),dn2(i)
    i = i + 1
endwhile
close,1
nt2 = i - 1 
oldn2 = oldn2(0:nt2-1)
newn2 = newn2(0:nt2-1)
dv2 = dv2(0:nt2-1)
dnv2 =dnv2(0:nt2-1)
diff2 = diff2(0:nt2-1)
v2 = v2(0:nt2-1)
dn2 = dn2(0:nt2-1)


print, "old n: ",mean(oldn), mean(oldn2)
print, "n: ",mean(oldn), mean(oldn2)
print, "dnv: ",mean(oldn), mean(oldn2)
print, "diff: ",mean(oldn), mean(oldn2)
print, ": ",mean(oldn), mean(oldn2)
print, "old n: ",mean(oldn), mean(oldn2)
print, "old n: ",mean(oldn), mean(oldn2)


end
