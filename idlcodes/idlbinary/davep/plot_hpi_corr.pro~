filename = 'power_2005.txt'
openr, 1, filename

done = 0
t = ''
while not done do begin
   readf,1,t
   if strpos(t,'normalizing') ge 0 then done = 1
end while

readf,1,t
readf,1,t
year = fix(t)
rtime = intarr(50000)
time = rtime
str = strarr(50000)
hp = fltarr(50000)
hpi = time
fac = hp
itime = 0
while not eof(1) do begin
   readf,1,t
   str(itime) = strmid(t,0,10)
   time(itime) = fix(strmid(t,10,7))
   hp = float(strmid(t,17,8))
   hpi = fix(strmid(t,25,3))
   fac = float(strmid(t,28,8))

itime = itime + 1
stop
endwhile



end
   
