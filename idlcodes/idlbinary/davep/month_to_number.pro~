function month_to_number, month
  month = strlowcase(month)
  if strpos(month, 'jan') ge 0 then return '01'
  if strpos(month, 'feb') ge 0 then return '02'
  if strpos(month, 'mar') ge 0 then return '03'
  if strpos(month, 'apr') ge 0 then return '04'
  if strpos(month, 'may') ge 0 then return '05'
  if strpos(month, 'jun') ge 0 then return '06'
  if strpos(month, 'jul') ge 0 then return '07'
  if strpos(month, 'aug') ge 0 then return '08'
  if strpos(month, 'sep') ge 0 then return '09'
  if strpos(month, 'oct') ge 0 then return '10'
  if strpos(month, 'nov') ge 0 then return '11'
  if strpos(month, 'dec') ge 0 then return '12'
end
